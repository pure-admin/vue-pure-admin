    "@iconify-icons/ri": "^1.2.3",
    "@iconify/vue": "^4.0.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@pureadmin/theme": "^3.0.0",
    "@types/element-resize-detector": "1.1.3",
    "@types/intro.js": "^5.1.0",
    "@types/js-cookie": "^3.0.1",
    "@types/mockjs": "^1.0.7",
